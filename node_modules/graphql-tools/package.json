{
  "name": "graphql-tools",
  "version": "8.2.3",
  "description": "Useful tools to create and manipulate GraphQL schemas.",
  "sideEffects": false,
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/schema": "8.3.5",
    "tslib": "~2.3.0"
  },
  "optionalDependencies": {
    "@apollo/client": "~3.2.5 || ~3.3.0 || ~3.4.0 || ~3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-tools",
    "directory": "packages/graphql-tools"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "typings": "index.d.ts",
  "typescript": {
    "definition": "index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./*": {
      "require": "./*.js",
      "import": "./*.mjs"
    },
    "./package.json": "./package.json"
  }
}
